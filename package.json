{
  "name": "electra-one",
  "version": "1.3.1",
  "description": "Sequencer, Router & Instrument Files for Electra One",
  "main": "index.js",
  "bin": {
    "electra-one": "./index.js"
  },
  "scripts": {
    "router": "nodemon --ignore state index router",
    "list": "node index list",
    "page": "node index page",
    "info": "node index info",
    "upload": "node index upload",
    "remove": "node index remove",
    "bacara": "nodemon --verbose -L --ignore state index bacara",
    "fuse": "nodemon --verbose -L --ignore state index fuse",
    "virus": "nodemon --verbose -L --ignore state index virus companion",
    "fuse_bacara": "node index.js fuse  --custom custom_bacara.json",
    "setup_joris": "node index setup --custom custom_joris.json",
    "setup_richie": "node index setup --custom custom_richie.json",
    "preset": "node index preset --focus bacara --filename \"presets/Bacara.eproj\" --presetName \"Bacara\" --custom custom_bacara.json",
    "preset_virus_companion": "node index preset --focus virus --filename \"presets/Virus TI Companion.eproj\" --presetName \"Virus TI Companion\" --custom custom_joris.json",
    "preset_bacara_joris": "node index preset --focus bacara --filename \"presets/Bacara Joris.eproj\" --presetName \"Bacara Joris\" --custom custom_joris.json",
    "preset_bacara_richie": "node index preset --focus bacara --filename \"presets/Bacara Richie.eproj\" --presetName \"Bacara Richie\" --custom custom_richie.json",
    "build": "npm run preset;npm run setup_joris;npm run preset_virus_companion;npm run preset_bacara_joris;npm run preset_bacara_richie",
    "start": "NO_SEED=${NO_SEED:123} npm run bacara",
    "start_richie": "NO_SEED=${SEED:123} npm run fuse --custom custom_richie.json",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorisroling/electra-one.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jorisroling/electra-one/issues"
  },
  "homepage": "https://github.com/jorisroling/electra-one#readme",
