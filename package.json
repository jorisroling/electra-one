{
  "name": "electra-one",
  "version": "1.2.20",
  "description": "Sequencer, Router & Instrument Files for Electra One",
  "main": "index.js",
  "bin": {
    "electra-one": "./index.js"
  },
  "scripts": {
    "router": "nodemon --ignore state index router",
    "list": "node index list",
    "page": "node index page",
    "info": "node index info",
    "upload": "node index upload",
    "remove": "node index remove",
    "bacara": "nodemon --verbose -L --ignore state index bacara",
    "fuse": "nodemon --verbose -L --ignore state index fuse",
    "setup_joris": "node index setup --custom custom_joris.json",
    "setup_richie": "node index setup --custom custom_richie.json",
    "preset": "node index preset --filename \"presets/Bacara.epr\" --presetName \"Bacara\" --custom custom_bacara.json",
    "preset_joris": "node index preset --filename \"presets/Bacara Joris.epr\" --presetName \"Bacara Joris\" --custom custom_joris.json",
    "preset_richie": "node index preset --filename \"presets/Bacara Richie.epr\" --presetName \"Bacara Richie\" --custom custom_richie.json",
    "build": "npm run preset;npm run setup_joris;npm run preset_joris;npm run preset_richie",
    "start": "npm run fuse",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorisroling/electra-one.git"
  },
  "keywords": [],
